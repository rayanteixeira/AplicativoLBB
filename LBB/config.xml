<widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="com.devexpress.apptemplate" versionCode="1" version="1.0.0">
  <name>LBB</name>
  <description>Aplicativo do Laboratório Beneficiente de Belém</description>
  <author href="" email="rayanteixeira91@gmail.com">Rayan Teixeira</author>
  <preference name="android-minSdkVersion" value="14" />
  <preference name="android-targetSdkVersion" value="14" />
  <preference name="android-windowSoftInputMode" value="stateUnspecified" />
  <!--ios-->
  <preference name="DisallowOverscroll" value="true" />
  <preference name="HideKeyboardFormAccessoryBar" value="false" />
  <!--<preference name="phonegap-version" value="3.7.0" />-->
  <preference name="phonegap-version" value="5.0.0" />
  <preference name="permissions" value="none" />
  <preference name="prerendered-icon" value="true" />
  <preference name="fullscreen" value="false" />
  <preference name="KeyboardShrinksView" value="true" />
  <preference name="SplashScreen" value="splash" />
  <preference name="SplashScreenDelay" value="3000" />
  <preference name="AutoHideSplashScreen" value="false" />
  <preference name="DisallowOverscroll" value="true" />
  <preference name="StatusBarOverlaysWebView" value="false" />
  <preference name="StatusBarBackgroundColor" value="#000000" />
  <feature name="http://api.phonegap.com/1.0/network" />
  <feature name="http://api.phonegap.com/1.0/geolocation" />
  <preference name="xwalkZOrderOnTop" value="true" />
  <gap:plugin name="org.apache.cordova.splashscreen" version="0.3.4" onload="true" />
  <gap:plugin name="cordova-plugin-statusbar" source="npm" version="1.0.1" onload="true" />
  <gap:plugin name="com.devexpress.plugins.devextremeaddon" version="1.0.1" />
  <gap:plugin name="cordova-plugin-ios-longpress-fix" version="1.1.0" source="npm" />
  <gap:plugin name="org.apache.cordova.inappbrowser" version="0.5.2" />
  <gap:plugin name="org.apache.cordova.geolocation" version="0.3.10" />
